<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.springblade.modules.uni.mapper.RouteMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="routeResultMap" type="org.springblade.modules.uni.entity.Route">
        <id column="id" property="id"/>
        <result column="title" property="title"/>
        <result column="text" property="text"/>
        <result column="img" property="img"/>
        <result column="path" property="path"/>
    </resultMap>


    <select id="selectRoutePage" resultMap="routeResultMap">
        select * from common_route where is_deleted = 0
    </select>

</mapper>
